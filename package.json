{

  "private": true,
  "scripts": {
    "start": "set NODE_OPTIONS=--openssl-legacy-provider && umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "axios": "^1.2.1",
    "draft-js": "^0.11.7",
    "draftjs-to-html": "^0.9.1",
    "js-cookie": "^3.0.1",
    "jsonwebtoken": "^9.0.0",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.4",
    "nprogress": "^0.2.0",
    "react": "17.x",
    "react-dom": "17.x",
    "react-draft-wysiwyg": "^1.15.0",
    "react-particles": "^2.6.0",
    "react-tsparticles": "^2.6.0",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.4.2",
    "tsparticles": "^2.6.0",
    "umi": "^3.5.35"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.35",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
